   "source": [
    "activo=\"accion1\" #definimos activo como accion1\n",
    "if activo==\"accion1\": #creamos un if en el que sabemos que si va a pertenecer \n",
    "  r=r1\n",
    "plt.plot(r*100,\"b\")\n",
    "plt.axhline(y=np.mean(r)*100,color=\"g\") #definimos la linea que pasa por la media de todos los datos obtenidos\n",
    "plt.axhline(y=(np.mean(r)+np.std(r))*100,color=\"r\")# definimos la desviacion estandar que pasa por encima de los datos\n",
    "plt.axhline(y=(np.mean(r)-np.std(r))*100,color=\"r\")# definimos la desviacion estandar que pasa por debajo de los datos\n",
    "plt.title(\"AAPL\")\n",
    "plt.xlabel(\"Dias\"),plt.ylabel(\"Redimiento diario (%)\") # creamos los nombres de (x) y (y)\n",
    "plt.grid()"
   ]
